{
  "name": "powerpuff-pong",
  "version": "1.0.0",
  "description": "Powerpuff Girls themed Pong game",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "dev": "tsc --watch & npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "serve": "python3 -m http.server 8000"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "pong",
    "game",
    "powerpuff-girls",
    "typescript",
    "tailwind"
  ],
  "author": "",
  "license": "MIT"
}
